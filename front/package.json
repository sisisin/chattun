{
  "name": "chattun-front",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^14.16.0",
    "yarn": "^1.22.5"
  },
  "dependencies": {
    "emoji-mart": "2.11.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-scripts": "3.4.0",
    "stylelint": "13.2.0",
    "stylelint-color-format": "1.0.0",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-recess-order": "2.0.3",
    "stylelint-order": "4.0.0",
    "stylelint-prettier": "1.1.2"
  },
  "scripts": {
    "start": "run-p start-js watch-css watch-sw",
    "start-js": "react-scripts start",
    "start-nm": "run-p start-js-nm watch-css",
    "start-js-nm": "GENERATE_SOURCEMAP=false react-scripts start",
    "build": "run-s build-css build-sw build-js",
    "build-js": "react-scripts build && rm -rf ../server/public && mv build ../server/public",
    "build-sw": "tsc -p ./swSrc",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "_lint": "eslint --ext js,jsx,ts,tsx .",
    "lint": "yarn _lint",
    "lint-fix": "yarn _lint --fix",
    "stylelint": "stylelint \"./src/app/css/**/*.css\" ",
    "stylelint-fix": "stylelint \"./src/app/css/**/*.css\" --fix",
    "test-all": "yarn format-check && yarn test",
    "_prettier": "prettier \"src/**/*.{json,css,scss}\"",
    "format": "yarn lint-fix && yarn _prettier --write",
    "format-check": "yarn lint && yarn _prettier --list-different",
    "storybook": "start-storybook -p 9009 -s ./public",
    "build-storybook": "build-storybook -s ./public",
    "build-css": "postcss src/app/css/main.css -o public/index.css",
    "watch-css": "postcss src/app/css/main.css -o public/index.css -w",
    "watch-sw": "yarn build-sw -w",
    "test-visual": "reg-suit run",
    "pretest-visual": "storycap --serverCmd \"yarn storybook\" http://localhost:9009 -o actual_img --serverTimeout 50000",
    "sketch": "story2sketch --url http://localhost:9009/iframe.html --output stories.asketch.json",
    "g": "yarn plop feature",
    "gm": "yarn plop module"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.css": [
      "yarn stylelint-fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "7.8.6",
    "@loadable/component": "5.12.0",
    "@slack/web-api": "6.0.0",
    "@storybook/addon-actions": "5.3.14",
    "@storybook/addon-links": "5.3.14",
    "@storybook/addons": "5.3.14",
    "@storybook/preset-create-react-app": "1.5.2",
    "@storybook/react": "5.3.14",
    "@types/emoji-mart": "2.11.3",
    "@types/jest": "25.1.3",
    "@types/loadable__component": "5.10.0",
    "@types/markdown-it": "0.0.9",
    "@types/node": "13.7.7",
    "@types/node-emoji": "1.8.1",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-outside-click-handler": "1.3.0",
    "@types/react-router": "5.1.4",
    "@types/react-router-dom": "5.1.3",
    "@types/socket.io-client": "1.4.32",
    "@types/storybook__addon-actions": "5.2.1",
    "@types/storybook__addon-info": "5.2.1",
    "@types/storybook__addon-links": "5.2.1",
    "@types/storybook__react": "5.2.1",
    "autoprefixer": "9.7.4",
    "babel-loader": "8.0.6",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-jest": "23.8.1",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "markdown-it": "10.0.0",
    "node-emoji": "1.10.0",
    "npm-run-all": "4.1.5",
    "plop": "2.5.4",
    "postcss-cli": "7.1.0",
    "postcss-custom-media": "7.0.8",
    "postcss-custom-properties": "9.1.1",
    "postcss-import": "12.0.1",
    "postcss-nested": "4.2.1",
    "postcss-nesting": "7.0.1",
    "prettier": "1.19.1",
    "react-docgen-typescript-webpack-plugin": "1.1.0",
    "react-hook-form": "5.1.1",
    "react-outside-click-handler": "1.3.0",
    "react-router-dom": "5.1.2",
    "reg-keygen-git-hash-plugin": "0.10.16",
    "reg-notify-github-plugin": "0.8.5",
    "reg-publish-s3-plugin": "0.9.0",
    "reg-suit": "0.10.0",
    "rxjs": "6.5.4",
    "slack": "11.0.2",
    "socket.io-client": "2.3.0",
    "story2sketch": "1.7.0",
    "storycap": "2.3.5",
    "typeless": "1.5.0",
    "typescript": "3.8.3"
  },
  "resolutions": {
    "@types/react": "16.9.23"
  },
  "proxy": "http://localhost:3100"
}
